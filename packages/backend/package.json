{
  "name": "@toshik-babe/backend",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "main": "src/index.ts",
  "scripts": {
    "dev": "bun --env-file=../../.env --hot src/index.ts",
    "build": "bun build src/index.ts --outdir dist --target bun",
    "start": "cross-env NODE_EXTRA_CA_CERTS=./certs/russian_trusted_root_ca.pem bun --env-file=../../.env src/index.ts",
    "lint": "eslint src/",
    "format": "prettier --write src/",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@ai-sdk/google": "^3.0.22",
    "@ai-sdk/openai": "^3.0.26",
    "@toshik-babe/shared": "workspace:*",
    "ai": "^6.0.77"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "cross-env": "^7.0.3"
  },
  "peerDependencies": {
    "typescript": "^5"
  }
}
