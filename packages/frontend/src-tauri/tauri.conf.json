{
  "$schema": "https://raw.githubusercontent.com/tauri-apps/tauri/dev/crates/tauri-cli/schema.json",
  "productName": "Toshik Babe Engine",
  "version": "0.1.0",
  "identifier": "com.toshik-babe.engine",
  "build": {
    "frontendDist": "../dist",
    "devUrl": "http://localhost:1420",
    "beforeDevCommand": "cd .. && bun run dev:web",
    "beforeBuildCommand": "cd .. && bun run build:web"
  },
  "app": {
    "title": "Toshik Babe Engine",
    "windows": [
      {
        "title": "Toshik Babe Engine",
        "width": 1200,
        "height": 800,
        "resizable": true,
        "fullscreen": false,
        "minWidth": 800,
        "minHeight": 600
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}
